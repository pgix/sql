//*******select command basics*****///

select * from bigquery-public-data.new_york_citibike.citibike_trips where gender = 'female'
select * from bigquery-public-data.new_york_citibike.citibike_trips where gender = 'male'
select * from bigquery-public-data.new_york_citibike.citibike_trips
SELECT tripduration FROM `bigquery-public-data.new_york_citibike.citibike_trips` where tripduration >= 500 select * from bigquery-public-data.new_york_citibike.citibike_trips
SELECT tripduration FROM `bigquery-public-data.new_york_citibike.citibike_trips` where tripduration >= 500 select * from `bigquery-public-data.new_york_citibike.citibike_trips' where gender = 'male'
SELECT tripduration FROM `bigquery-public-data.new_york_citibike.citibike_trips` where tripduration >= 500 select * from `bigquery-public-data.new_york_citibike.citibike_trips' LIMIT 10
SELECT tripduration FROM `bigquery-public-data.new_york_citibike.citibike_trips` where tripduration >= 500
SELECT * FROM `bigquery-public-data.new_york_citibike.citibike_trips` LIMIT 10

//******sum, avg, count, max, min etc*****//
SELECT count(number_of_strikes) FROM bigquery-public-data.noaa_lightning.lightning_2019 where number_of_strikes >= 100
SELECT number_of_strikes FROM bigquery-public-data.noaa_lightning.lightning_2019 where number_of_strikes >= 100
SELECT min(number_of_strikes) FROM bigquery-public-data.noaa_lightning.lightning_2019 LIMIT 1000
SELECT max(number_of_strikes) FROM bigquery-public-data.noaa_lightning.lightning_2019 LIMIT 1000
SELECT count(number_of_strikes) FROM bigquery-public-data.noaa_lightning.lightning_2019 LIMIT 1000
SELECT avg(number_of_strikes) FROM bigquery-public-data.noaa_lightning.lightning_2019 LIMIT 1000
SELECT sum(number_of_strikes) FROM bigquery-public-data.noaa_lightning.lightning_2019 LIMIT 1000
SELECT * FROM bigquery-public-data.noaa_lightning.lightning_2019 LIMIT 1000
