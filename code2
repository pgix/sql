SELECT count(number_of_strikes) FROM bigquery-public-data.noaa_lightning.lightning_2019 where number_of_strikes >= 100
SELECT number_of_strikes FROM bigquery-public-data.noaa_lightning.lightning_2019 where number_of_strikes >= 100
SELECT min(number_of_strikes) FROM bigquery-public-data.noaa_lightning.lightning_2019 LIMIT 1000
SELECT max(number_of_strikes) FROM bigquery-public-data.noaa_lightning.lightning_2019 LIMIT 1000
SELECT count(number_of_strikes) FROM bigquery-public-data.noaa_lightning.lightning_2019 LIMIT 1000
SELECT avg(number_of_strikes) FROM bigquery-public-data.noaa_lightning.lightning_2019 LIMIT 1000
SELECT sum(number_of_strikes) FROM bigquery-public-data.noaa_lightning.lightning_2019 LIMIT 1000
SELECT * FROM bigquery-public-data.noaa_lightning.lightning_2019 LIMIT 1000
